#define size 10
#define max nums [0]
int main(void)
{
    int nums[size];
    printf("Enter the numbers\n");
    for (int i=0;i<10; i++){
            scanf("%d",&nums[i]);
    }

    for (int i=0;i<10; i++){
            if(nums[i]>max){
                max=nums[i];
    }
    else{
        continue;
}
}
  printf("Largest Value: %d\n",max);
  return 0;
}
